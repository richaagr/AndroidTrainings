<manifest
    package="com.thoughtworks.remoteservicedemo"
    xmlns:android="http://schemas.android.com/apk/res/android">

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <service android:name=".services.MyRemoteService">
      <intent-filter>
        <action android:name="com.thoughtworks.remoteservicedemo.remoteobjects.MyRemoteObject"/>
      </intent-filter>
    </service>
  </application>

</manifest>
